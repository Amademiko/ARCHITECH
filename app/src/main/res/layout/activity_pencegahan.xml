<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pencegahan"
    android:background="@drawable/bgappss">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="323dp"
        android:layout_height="91dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="10dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hpencegahan" />

    <TextView
        android:id="@+id/pencegahanView"
        android:layout_width="322dp"
        android:layout_height="323dp"
        android:layout_above="@+id/keMainMenu"
        android:layout_below="@+id/imageView"
        android:layout_alignStart="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_marginStart="1dp"
        android:layout_marginTop="66dp"
        android:textSize="20dp" />

    <Button
        android:id="@+id/keMainMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="31dp"
        android:text="Menu Utama"
        android:onClick="menuuLayout"/>

</RelativeLayout>